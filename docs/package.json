{
  "name": "caddy-ingress-controller-docs",
  "private": true,
  "description": "Caddy Ingress Controller docs",
  "version": "0.1.1",
  "engines": {
    "node": ">=16.15.0"
  },
  "browserslist": [
    "defaults"
  ],
  "license": "MIT",
  "scripts": {
    "start": "run clean && hugo server  --bind=0.0.0.0 --disableFastRender",
    "build": "run clean && hugo --gc --minify",
    "build:preview": "run build -D -F",
    "clean": "rimraf public resources",
    "lint": "run lint:styles && run lint:markdown",
    "lint:styles": "stylelint \"assets/scss/**/*.{css,sass,scss,sss,less}\"",
    "lint:markdown": "markdownlint-cli2 \"*.md\" \"content/**/*.md\"",
    "lint:markdown-fix": "markdownlint-cli2-fix \"*.md\" \"content/**/*.md\"",
    "server": "hugo server",
    "test": "run -s lint",
    "env": "env",
    "check": "yarn version && hugo version",
    "copy:katex-fonts": "cp ./node_modules/katex/dist/fonts/* ./static/fonts/"
  },
  "devDependencies": {
    "@babel/cli": "^7.17",
    "@babel/core": "^7.17",
    "@babel/preset-env": "^7.17",
    "@fullhuman/postcss-purgecss": "^4.1",
    "@hyas/doks": "^0.4",
    "autoprefixer": "^10.4",
    "bootstrap": "^5.1",
    "clipboard": "^2.0",
    "eslint": "^8.15",
    "flexsearch": "^0.7.21",
    "highlight.js": "^11.5",
    "hugo-extended": "^0.99.1",
    "instant.page": "^5.1",
    "katex": "^0.15",
    "lazysizes": "^5.3",
    "markdownlint-cli2": "^0.4.0",
    "netlify-plugin-submit-sitemap": "^0.3.0",
    "postcss": "^8.4",
    "postcss-cli": "^9.1",
    "purgecss-whitelister": "^2.4",
    "rimraf": "^3.0.2",
    "stylelint": "^14.8",
    "stylelint-config-standard-scss": "^3.0"
  },
  "dependenciesMeta": {
    "@hyas/doks": {
      "built": false
    }
  }
}
